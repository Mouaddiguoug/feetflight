{
  "name": "typescript-elysia-starter",
  "version": "0.0.0",
  "description": "TypeScript + Elysia API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "bun run build && NODE_ENV=production bun dist/server.js",
    "dev": "NODE_ENV=development bun --watch src/server.ts",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "ts:check": "tsc --noEmit --project .",
    "test": "bun --bun jest --forceExit --detectOpenHandles",
    "test:node": "NODE_OPTIONS='--experimental-vm-modules' node ./node_modules/jest/bin/jest.js --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "bun run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "vercel-build": "echo hello"
  },
  "dependencies": {
    "@elysiajs/bearer": "latest",
    "@elysiajs/cors": "latest",
    "@elysiajs/jwt": "latest",
    "@elysiajs/openapi": "latest",
    "@elysiajs/static": "latest",
    "@labzzhq/compressor": "latest",
    "@react-email/components": "^0.5.7",
    "@react-email/render": "^1.4.0",
    "@vonage/server-sdk": "^3.11.0",
    "aws-sdk": "^2.1329.0",
    "bcrypt": "^5.1.0",
    "crypto": "^1.0.1",
    "elysia": "latest",
    "elysia-helmet": "latest",
    "fcm": "^1.0.3",
    "firebase-admin": "^11.11.0",
    "handlebars": "^4.7.8",
    "moment": "^2.29.4",
    "neo4j-driver": "^5.6.0",
    "openai": "^6.8.1",
    "otp-generator": "^4.0.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native-progress-steps": "^1.3.4",
    "resend": "^6.4.1",
    "stripe": "^11.15.0",
    "uid": "^2.0.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "bun-types": "latest",
    "@swc/core": "^1.2.220",
    "@types/bcrypt": "^5.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/otp-generator": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.0.11",
    "lint-staged": "^13.0.3",
    "node-config": "^0.0.2",
    "node-gyp": "^9.1.0",
    "pm2": "^5.3.0",
    "prettier": "^3.2.5"
  },
  "trustedDependencies": [
    "@swc/core",
    "aws-sdk",
    "bcrypt",
    "protobufjs"
  ]
}
